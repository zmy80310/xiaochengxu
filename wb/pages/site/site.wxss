/* pages/site/site.wxss */
/* pages/home/getaddress/getaddress.wxss */
page{
  background: rgba(233, 232, 232, 0.719);
}
.list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  background: #fff;
  margin: 15px 0;
  padding: 13px 10px;
}
.row1,.address{
  line-height: 20px;
}
.user_info{
  width: 353px;
  font-size: 14px;
  color: #333;
  position: relative;
  margin-bottom: 30rpx;
}
.name{
  margin: 0 8px;
}
.change{
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
    position: absolute;
    top:17rpx;
    right:15rpx;
}
.change-img{
  width: 50px;
  height: 20px;
  margin-right: -20rpx;
  margin-top:20rpx;
  color: blue;


}
.change-img>image{
  display: block;
  width: 100%;
  height: 100%;
}
.address{
  margin-top: 5px;
}
.newAddres{
  color: #fff;
  background: #ff6700;
  width: 100%;
  line-height: 50px;
  text-align: center;
  position: absolute;
  bottom: 0;
}
.move{
  margin-top:20rpx;
  color:#f00;
}

